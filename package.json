{
  "name": "highscore",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --kill-others \"tsc -w\" \"nodemon dist/server.js -e js\"",
    "debug": "concurrently --kill-others \"tsc -w\" \"nodemon --inspect dist/server.js -e js\"",
    "test:watch": "mocha -r ts-node/register -w ./spec/**/*.spec.ts --watch-extensions ts",
    "test": "mocha -r ts-node/register -w ./spec/**/*.spec.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "format": "prettier --write 'src/**/*ts'",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\"",
    "migrate": "knex migrate:latest"
  },
  "dependencies": {
    "body-parser": "1.19.1",
    "cors": "2.8.5",
    "dotenv": "14.2.0",
    "errorhandler": "1.5.1",
    "express": "4.17.2",
    "express-winston": "4.2.0",
    "http-status-codes": "2.2.0",
    "knex": "1.0.1",
    "moment": "2.29.1",
    "pg": "8.7.1",
    "pg-promise": "10.11.1",
    "winston": "3.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.12",
    "@types/dotenv": "8.2.0",
    "@types/errorhandler": "1.5.0",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "concurrently": "7.0.0",
    "eslint": "8.7.0",
    "nodemon": "2.0.15",
    "typescript": "4.5.5"
  },
  "engines": {
    "yarn": "1.22.17",
    "node": "16.13.2"
  },
  "packageManager": "yarn@3.1.1"
}
